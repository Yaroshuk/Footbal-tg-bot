{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "standard"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-useless-catch": "off",
    "space-before-function-paren": "off",
    "prettier/prettier": "error",
    "comma-dangle": "off"
  }
}
